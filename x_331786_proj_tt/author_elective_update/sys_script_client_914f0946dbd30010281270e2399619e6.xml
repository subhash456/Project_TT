        <condition/>
        <description>Loading the Subjects into Subject # fields when the stream is selected.</description>
        <field>stream_sel</field>
        <global>true</global>
        <isolate_script>true</isolate_script>
        <messages/>
        <name>Subjects Load</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading, isTemplate) {
    if (isLoading || newValue === '') {
        return;
    }
	var styr=g_form.getValue('stream_sel');
    var k = new GlideAjax('getCourses');
    k.addParam('sysparm_name', 'Courses');
	k.addParam('stream',styr);
    k.getXML(getc);

    function getc(response) {
        var y = response.responseXML.documentElement.getAttribute('answer');
        var fields = [];
        fields = JSON.parse(y);
        var i = 1;
        for (var count = 0; count < fields.length; count++) {
            g_form.setValue('subject'+i, fields[count]);
            i++;
		}
    }

    //Type appropriate comment here, and begin script below

}]]></script>
